import java.util.Random;

public class ex1 {

    public static void main(String[] args) {
        Random gerador = new Random();
        int[][] matriz = new int[3][3];

        for(int i=0; i < 3;i++){
            for(int j=0; j <3;j++){
                matriz[i][j] = gerador.nextInt(100);
            }
        }
        System.out.println(matriz[0][0]+matriz[1][1]+matriz[2][2]);
    }
}
